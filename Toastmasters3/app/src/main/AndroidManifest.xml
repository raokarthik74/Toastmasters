<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.tmdistrict92.toastmasters" >

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="com.google.android.c2dm.permission.RECEIVE" />

    <!--
      IMPORTANT: Change "com.parse.starter.permission.C2D_MESSAGE" in the lines below
      to match your app's package name + ".permission.C2D_MESSAGE".
    -->
    <permission
        android:name="org.tmdistrict92.toastmasters.permission.C2D_MESSAGE"
        android:protectionLevel="signature" />

    <uses-permission android:name="org.tmdistrict92.toastmasters.permission.C2D_MESSAGE" />

    <application
        android:screenOrientation="portrait"
        android:name=".ParseNotification"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:launchMode="singleTop"
        android:theme="@style/AppTheme" >
        <activity android:screenOrientation="portrait"
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:screenOrientation="portrait"
            android:name=".timerSelectionActivity"
            android:label="@string/title_activity_timer_selection"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="org.tmdistrict92.toastmasters.MainActivity" />
        </activity>
        <activity android:screenOrientation="portrait"
            android:name=".speechTimer"
            android:label="@string/title_activity_speech_timer"
            android:parentActivityName=".timerSelectionActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="org.tmdistrict92.toastmasters.timerSelectionActivity" />
        </activity>
        <activity android:screenOrientation="portrait"
            android:name=".AhCounterActivity"
            android:label="@string/title_activity_ah_counter"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="org.tmdistrict92.toastmasters.MainActivity" />
        </activity>
        <activity android:screenOrientation="portrait"
            android:name=".roleDetail"
            android:label="@string/title_activity_role_detail"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="org.tmdistrict92.toastmasters.MainActivity" />
        </activity>
        <activity android:screenOrientation="portrait"
            android:name=".webView"
            android:label="@string/title_activity_web_view"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="org.tmdistrict92.toastmasters.MainActivity" />
        </activity>
        <activity android:screenOrientation="portrait"
            android:name=".DataSaveActivity"
            android:label="@string/title_activity_data_save"
            android:parentActivityName=".AhCounterActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="org.tmdistrict92.toastmasters.AhCounterActivity" />
        </activity>
        <activity android:screenOrientation="portrait"
            android:name=".TimerReport"
            android:label="@string/title_activity_timer_report"
            android:parentActivityName=".speechTimer" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="org.tmdistrict92.toastmasters.speechTimer" />
        </activity>
        <activity android:screenOrientation="portrait"
            android:name=".AdvancedSpeechesTimerSetting"
            android:label="@string/title_activity_advanced_speeches_timer_setting"
            android:parentActivityName=".timerSelectionActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="org.tmdistrict92.toastmasters.timerSelectionActivity" />
        </activity>

        <service android:name="com.parse.PushService" />

        <receiver
            android:name=".util.Receiver"
            android:exported="false" >
            <intent-filter>
                <action android:name="com.parse.push.intent.RECEIVE" />
                <action android:name="com.parse.push.intent.DELETE" />
                <action android:name="com.parse.push.intent.OPEN" />
            </intent-filter>
        </receiver>
        <receiver
            android:name="com.parse.GcmBroadcastReceiver"
            android:permission="com.google.android.c2dm.permission.SEND" >
            <intent-filter>
                <action android:name="com.google.android.c2dm.intent.RECEIVE" />
                <action android:name="com.google.android.c2dm.intent.REGISTRATION" />

                <!-- IMPORTANT: Change "com.parse.starter" to match your app's package name. -->
                <category android:name="org.tmdistrict92.toastmasters" />
            </intent-filter>
        </receiver>

        <meta-data
            android:name="com.parse.push.notification_icon"
            android:resource="@mipmap/ic_launcher" />

        <activity android:screenOrientation="portrait"
            android:name=".DistrictSelectionActivity"
            android:label="@string/title_activity_district_selection" >
        </activity>
        <activity android:screenOrientation="portrait"
            android:name=".WebactivityForParse"
            android:label="@string/title_activity_webactivity_for_parse" >
        </activity>
        <activity android:screenOrientation="portrait"
            android:name=".PushNotificationActivity"
            android:label="@string/title_activity_push_notification" >
        </activity>
        <activity android:screenOrientation="portrait"
            android:name=".LoginActivity"
            android:label="@string/title_activity_login" >
        </activity>
    </application>

</manifest>
